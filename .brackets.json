{
    "sass.enabled": false,
    "sass.compiler": "libsass",
    "path": {
        "src/sass/*.sass": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css",
                "imagePath": null,
                "sourceComments": false,
                "outputStyle": "nested"
            },
            "linting.collapsed": false,
            "brackets-git.gitPath": "git"
        }
    }
}
